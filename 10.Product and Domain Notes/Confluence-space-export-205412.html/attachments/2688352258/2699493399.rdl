    <DataSet Name="CloseDate_LookUp">
      <Query>
        <DataSourceName>NewIssues_Prod</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IssueName">
            <Value>=Parameters!IssueName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>------For Issues Close Date look up, created a Depency for user to select if they needed
SELECT DISTINCT
    IssueName,
    CloseDate
FROM [SQLCIP].[NewIssues].dbo.Issue
WHERE IssueName = @IssueName
order by CloseDate desc</CommandText>
      </Query>
