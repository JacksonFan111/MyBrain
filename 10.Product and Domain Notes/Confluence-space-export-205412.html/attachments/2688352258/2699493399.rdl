    <DataSet Name="AdvisorName_LookUp">
      <Query>
        <DataSourceName>NewIssues_Prod</DataSourceName>
        <CommandText>--For AdvisorName Look Up -- SSRS
SELECT DISTINCT su.FullName AS AdvisorName
FROM 
    [Dynamics].[CRM_MSCRM].[dbo].[dsl_advisercommissioncode] acc
INNER JOIN 
    [Dynamics].[CRM_MSCRM].[dbo].[dsl_advisercommissionlink] acl 
    ON acl.dsl_AdvisersId = acc.dsl_advisercommissioncodeId
INNER JOIN 
    [Dynamics].[CRM_MSCRM].[dbo].[SystemUser] su 
    ON su.SystemUserId = acl.dsl_Adviser
WHERE 
    acc.dsl_Retired = 0 
    AND acl.statecode = 0 
    AND acc.dsl_name &lt;&gt; 'HEADOFFICE'
UNION 
SELECT '&lt;All Advisors&gt;' 
ORDER BY AdvisorName;</CommandText>
      </Query>
      <Fields>
        <Field Name="AdvisorName">
          <DataField>AdvisorName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
